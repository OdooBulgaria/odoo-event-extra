<?xml version="1.0"?>
<openerp>
<data noupdate="1">

    <record id="diploma_report_id" model="ir.actions.report.xml">
        <field name="name">Participant Diploma</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">event.participant</field>
        <field name="report_name">diploma_report</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
<!--        <field name="out_format">oo-pdf</field> -->
        <field name="parser_loc">event_participant_diploma/report/parser.py</field>
        <field name="report_rml">event_participant_diploma/report/template.odt</field>
        <field name="parser_state">loc</field>
        <field name="tml_source">file</field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['event.participant']" name="models"/>
        <field name="name">Diploma</field>
        <field eval="'ir.actions.report.xml,'+str(diploma_report_id)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>

</data>
</openerp>

